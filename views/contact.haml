%h1 Contact form

%form{:action => "/contact", :method => "post", :enctype => "multipart/form-data"}
    %label{:for => "name"} Name:
    %input{:type => "text", :name => "name"}
    %label{:for => "email"} Email:
    %input{:type => "text", :name => "email"}
    %input{:type => "submit", :value => "Submit"}
